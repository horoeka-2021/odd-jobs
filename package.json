{
  "name": "study-react",
  "version": "0.0.0",
  "private": true,
  "description": "Boilerplate for React/Express web applications",
  "scripts": {
    "webpack": "webpack --config client/webpack.config.js",
    "test": "jest --watch --noStackTrace",
    "lint": "eslint --ext .js,.jsx .",
    "dev": "run-p dev-client dev-server",
    "dev-client": "npm run webpack -- --watch --mode development",
    "dev-server": "NODE_LOG_LEVEL=dev node index ",
    "start": "npm run prod-server",
    "prod-server": "NODE_LOG_LEVEL=tiny node index ",
    "build": "npm run webpack -- --mode production"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-handlebars": "^5.3.4",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "babel-loader": "^8.2.3",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.0",
    "supertest": "^6.1.6",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  }
}
