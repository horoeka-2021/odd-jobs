{
  "name": "study-react",
  "version": "0.0.0",
  "private": true,
  "description": "Boilerplate for React/Express web applications",
  "scripts": {
    "test": "jest --watch --noStackTrace",
    "lint": "eslint --ext .js,.jsx .",
    "dev": "run-p dev-client dev-server",
    "dev-client": "webpack --config client/webpack.dev.js --watch",
    "dev-server": "NODE_LOG_LEVEL=dev nodemon server",
    "start": "npm run prod-server",
    "prod-server": "NODE_LOG_LEVEL=tiny node server",
    "build": "webpack --config client/webpack.prod.js"
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-handlebars": "^5.3.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
    "eslint-config-eda": "^0.1.4",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.0",
    "supertest": "^6.1.6",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  }
}
